<%- include header %>
<form method="post">
	Title: <br/>
<input type="text" name="title" value="<%= post.title %>" disable="disable"/><br/>
	Tag:<br/>
<input type="text" name="tag1" value="<%= post.tags[0] %>" disable="disable"/>
<input type="text" name="tag2" value="<%= post.tags[1] %>" disable="disable"/>
<input type="text" name="tag3" value="<%= post.tags[2] %>" disable="disable"/>
<br/>
	Content:<br/>
<textarea name="post" rows="20" cols="100"><%= post.post %></textarea><br/>
<input type="submit" value="Store"/>
</form>
<%- include footer %>